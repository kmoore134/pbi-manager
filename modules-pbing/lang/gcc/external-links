#etc/rc.d/servfoo       etc/rc.d/servfoo        keep
#include/libfoo.h       include/libfoo.h        replace
#bin/appfoo             bin/appfoo              binary,nocrash
#bin/appfoo2            bin/appfoo-test         binary

