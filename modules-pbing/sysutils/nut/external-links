bin/upscmd     bin/upscmd     binary,nocrash
bin/upslog     bin/upslog     binary,nocrash
bin/upsrw     bin/upsrw     binary,nocrash
bin/upssched-cmd     bin/upssched-cmd     binary,nocrash
sbin/upsd     sbin/upsd     binary,nocrash
sbin/upsmon     sbin/upsmon     binary,nocrash
sbin/upssched     sbin/upssched     binary,nocrash
